APP_NAME=CU Orchestrator
ENV=dev
LOG_LEVEL=INFO

# LLM (vLLM OpenAI-compatible endpoint)
LLM_BASE_URL=http://localhost:8001
LLM_MODEL=meta-llama/Llama-3.1-8B-Instruct
LLM_TEMPERATURE=0.2
LLM_MAX_TOKENS=512
LLM_TIMEOUT=60

# Routing thresholds
CLASSIFIER_MIN_CONF=0.55

# Agent toggles
ENABLE_POLICY_AGENT=true
ENABLE_TEXT2SQL_AGENT=true
ENABLE_RESEARCH_AGENT=true
